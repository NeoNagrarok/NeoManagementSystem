[?addContent]
<h2>
	Add content
</h2>
<form class="addContentForm" method="post" action="./">
	<label for="contentTitle">Content name : 
		<input type="text" id="contentTitle" name="contentTitle" />
	</label>
	<label for='contentOrder'>Order :
		<input type="number" id="contentOrder" name="contentOrder" />
	</label>
	<br />
	<button type="submit">Send</button>
</form>
